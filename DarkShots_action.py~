##### COUNTDOWN #####

count = []

k = -1

k += 1
count.append(0)
count[k] = Action()
count[k].index = k
count[k].text = "First, who looks at the clock and it shows X:00, X:10, X:20, ..."
count[k].expl = "Say: 'Shit, I forgot my cat in the dryer.' \nChoose someone, who have to finish his/her drink."

k += 1
count.append(0)
count[k] = Action()
count[k].index = k
count[k].text = "First, who finds a tippo in this game."
count[k].expl = "Say: 'Heiko, I will call your english teacher!' \nGive five sips."

k += 1
count.append(0)
count[k] = Action()
count[k].index = k
count[k].text = "First, who sees someone enters the room (not you)."
count[k].expl = "Spread your arms and say: 'Praise the shots'! \nShot for everyone."

k += 1
count.append(0)
count[k] = Action()
count[k].index = k
count[k].text = "First, who sees that someone has to drink three sips.."
count[k].expl = "Say: 'Save the trees, drink threes'! \nEveryone has to drink three sips."

k += 1
count.append(0)
count[k] = Action()
count[k].index = k
count[k].text = "First, who sees you get a virus (ingame)"
count[k].expl = "Say: 'God bless you!' \nGod bless you, drink twice."

k += 1
count.append(0)
count[k] = Action()
count[k].index = k
count[k].text = "First, who sees someone want to change the song"
count[k].expl = "Say: 'I want to hear Atemlos!' \nHear 'Atemlos' and give five sips."

k += 1
count.append(0)
count[k] = Action()
count[k].index = k
count[k].text = "First, who sees someone wants to leave the party"
count[k].expl = "Knee down and say:'THE END IS NEAR!' \nEveryone has to stay and drink."

